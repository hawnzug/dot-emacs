(defun flypy-re-build-regexp (str)
  (let* (first
         (exp
          (mapconcat
           (lambda (second)
             (if (and (>= second ?a) (<= second ?z))
                 (if first
                     (if-let ((chars (cdr (assoc (string first second) flypy-re--char-table))))
                         (prog1 (format "\\(?:%c%c\\|[%s]\\)" first second chars)
                           (setq first nil))
                       (prog1 (string first)
                         (setq first second)))
                   (prog1 ""
                     (setq first second)))
               (prog1 (regexp-quote (if first
                                        (string first second)
                                      (string second)))
                 (setq first nil))))
           str "")))
    (if first (concat exp (string first)) exp)))

